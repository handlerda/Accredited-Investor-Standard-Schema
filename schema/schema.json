{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "question": {
        "type": "string"
      },
      "response": {
        "type": "boolean"
      },
      "originalResponseDateTime": {
        "type": "string",
        "format": "date-time"
      },
      "latestResponseDateTime": {
        "type": "string",
        "format": "date-time"
      },
      "summarizedQuestion": {
        "type": "string"
      },
      "additionalDetails": {
        "type": "object",
        "properties": {
          "base64EncodedDocument": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "byte"
            }
          },
          "documentLink": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri"
            }
          },
          "freeText": {
            "type": "string"
          },
          "createdDateTime": {
            "type": "string",
            "format": "date-time"
          },
          "updatedDateTime": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "required": [
      "question",
      "response",
      "originalResponseDateTime",
      "latestResponseDateTime",
      "summarizedQuestion"
    ]
  }
}
